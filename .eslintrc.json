{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-use-before-define": "off",
    "no-plusplus": "off",
    "no-restricted-syntax": "off",
    "no-param-reassign": "off",
    "operator-linebreak": "off",
    "object-curly-newline": "off",
    "consistent-return": "off"
  }
}
